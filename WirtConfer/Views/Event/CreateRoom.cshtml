@model RoomViewModel


<form class="text-center" asp-controller="Event" asp-action="CreateRoom" method="post">
    <input asp-for="IdEvent" value="@Model.IdEvent" type="hidden" />
    <div id="Name">
        <label>Room name</label>
        <input asp-for="RoomName" />
        <span asp-validation-for="RoomName"></span>
    </div>
    <button type="submit" class="btn btn-primary">Save</button>
</form>
